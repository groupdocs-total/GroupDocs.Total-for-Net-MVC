  http://go.microsoft.com/fwlink/?LinkId=301879
  -->
<configuration>
  <configSections>
    <section name="signatureConfiguration" type="System.Configuration.NameValueSectionHandler" />
    <section name="applicationConfiguration" type="System.Configuration.NameValueSectionHandler" />
    <section name="serverConfiguration" type="System.Configuration.NameValueSectionHandler" />
    <section name="viewerConfiguration" type="System.Configuration.NameValueSectionHandler" />
    <section name="commonConfiguration" type="System.Configuration.NameValueSectionHandler" />
    <section name="annotationConfiguration" type="System.Configuration.NameValueSectionHandler" />
    <section name="comparisonConfiguration" type="System.Configuration.NameValueSectionHandler" />
    <section name="metadataConfiguration" type="System.Configuration.NameValueSectionHandler" />
  </configSections>
  <applicationConfiguration>
    <!--Absolute path to GroupDocs license file-->
    <add key="licensePath" value="C:/GroupDocs/Net/Total/GroupDocs.Total.NET.lic" />
  </applicationConfiguration>
  <serverConfiguration>
    <!--Host where to send API requests-->
    <add key="hostAddress" value="localhost" />
    <!--HTTP Port-->
    <add key="httpPort" value="8080" />
  </serverConfiguration>  
  <commonConfiguration>
     <!--Enable/disable page navigation elements-->
    <add key="isPageSelector" value="true" />   
    <!--Enable/disable document download-->
    <add key="isDownload" value="true" />
    <!--Enable/disable document upload-->
    <add key="isUpload" value="true" />
    <!--Enable/disable document print-->
    <add key="isPrint" value="true" />
    <!--Enable/disable documents browse-->
    <add key="isBrowse" value="true" />   
    <!--Enable/disable rewriting, if false uploaded document with name which exists will be saved as new copy -->
    <add key="isRewrite" value="true" />
  </commonConfiguration>  
  <viewerConfiguration>
    <!--Absolute or relative path to documents storage-->
    <add key="filesDirectory" value="C:/GroupDocs/Documents" />
    <!--Directory where all fonts files are stored-->
    <add key="fontsDirectory" value="" />
    <!--Default document to view-->
    <add key="defaultDocument" value="" />
    <!--Number of pages for preload, 0 for all-->
    <add key="preloadPageCount" value="0" />
    <!--Enable/disable zoom elements-->
    <add key="isZoom" value="true" />   
    <!--Enable/disable search-->
    <add key="isSearch" value="true" />
    <!--Enable/disable thumbnails-->
    <add key="isThumbnails" value="true" />
    <!--Enable/disable page rotate feature-->
    <add key="isRotate" value="true" />   
    <!--HTML or image rendering mode, false for image mode-->
    <add key="isHtmlMode" value="true" />
    <!--Enable/Disable cache storing-->
    <add key="cache" value="true" />
  </viewerConfiguration>
  <signatureConfiguration>
    <!--Absolute or relative path to documents storage-->
    <add key="filesDirectory" value="C:/GroupDocs/Documents" />
    <!--Directory where all signed files are stored-->
    <add key="outputDirectory" value="" />
    <!--Default document to view-->
    <add key="defaultDocument" value="" />
    <!--Number of pages for preload, 0 for all-->
    <add key="preloadPageCount" value="0" />
    <!--Directory used to store signatures data - default SignatureData-->
    <add key="dataDirectory" value="" />
    <!--Enable/disable text signature-->
    <add key="isTextSignature" value="true" />
    <!--Enable/disable image signature-->
    <add key="isImageSignature" value="true" />
    <!--Enable/disable digital signature-->
    <add key="isDigitalSignature" value="true" />
    <!--Enable/disable QR-Code signature-->
    <add key="isQrCodeSignature" value="true" />
    <!--Enable/disable BarCode signature-->
    <add key="isBarCodeSignature" value="true" />
    <!--Enable/disable stamp signature-->
    <add key="isStampSignature" value="true" />
    <!--Enable/disable download original document-->
    <add key="isDownloadOriginal" value="true" />
    <!--Enable/disable download signed document-->
    <add key="isDownloadSigned" value="true" />
  </signatureConfiguration>
  <annotationConfiguration>
    <!--Absolute or relative path to documents storage-->
    <add key="filesDirectory" value="C:/GroupDocs/Documents" />
    <!--Directory where all annotated files are stored-->
    <add key="outputDirectory" value="" />
    <!--Default document to view-->
    <add key="defaultDocument" value="" />
    <!--Number of pages for preload, 0 for all-->
    <add key="preloadPageCount" value="0" />
    <!--Directory used to store custom fonts-->
    <add key="fontsDirectory" value="" />
    <!--Enable/disable text annotation-->
    <add key="isTextAnnotation" value="true" />
    <!--Enable/disable area annotation-->
    <add key="isAreaAnnotation" value="true" />
    <!--Enable/disable point annotation-->
    <add key="isPointAnnotation" value="true" />
    <!--Enable/disable text strikeout annotation-->
    <add key="isTextStrikeoutAnnotation" value="true" />
    <!--Enable/disable polyline annotation-->
    <add key="isPolylineAnnotation" value="true" />
    <!--Enable/disable text field annotation-->
    <add key="isTextFieldAnnotation" value="true" />    
     <!--Enable/disable watermark annotation-->
    <add key="isWatermarkAnnotation" value="true" />
     <!--Enable/disable text replacement annotation-->
    <add key="isTextReplacementAnnotation" value="true" />
     <!--Enable/disable arrow annotation-->
    <add key="isArrowAnnotation" value="true" />
     <!--Enable/disable text redaction annotation-->
    <add key="isTextRedactionAnnotation" value="true" />
   <!--Enable/disable resource redaction annotation-->
    <add key="isResourcesRedactionAnnotation" value="true" />
      <!--Enable/disable text underline annotation-->
    <add key="isTextUnderlineAnnotation" value="true" />
   <!--Enable/disable distance annotation-->
    <add key="isDistanceAnnotation" value="true" />    
    <!--Enable/disable download original document-->
    <add key="isDownloadOriginal" value="true" />
    <!--Enable/disable download annotated document-->
    <add key="isDownloadAnnotated" value="true" />
  </annotationConfiguration>
  <comparisonConfiguration>
    <!--Absolute or relative path to documents storage-->
    <add key="filesDirectory" value="C:/GroupDocs/Documents/Comparison/" />
    <!--Directory where all compared files are stored-->
    <add key="resultDirectory" value="C:/GroupDocs/Documents/Comparison/compared" />
    <!-- Result pages preload. How many pages from a result document should be loaded, remaining pages will be loaded on page scrolling
    Set 0 to load all pages at once-->
    <add key="preloadResultPageCount" value="2" />
    <!--Enable/disable multi comparing feature-->
    <add key="isMultiComparing" value="true" />   
  </comparisonConfiguration>
  <metadataConfiguration>
    <!--Absolute or relative path to documents storage-->
    <add key="filesDirectory" value="C:/GroupDocs/Documents/Metadata/" />      
    <!--Enable/disable hidden data view feature-->
    <add key="isHidden" value="true" /> 
    <!--Enable/disable add custom data feature-->
    <add key="isCustom" value="true" />
    <!--Enable/disable multimedia files metadta reading feature-->
    <add key="isMultimedia" value="true" /> 
    <!--Enable/disable export to CSV-->
    <add key="isDownloadCsv" value="true" /> 
    <!--Enable/disable export to XML-->
    <add key="isDownloadXml" value="true" /> 
  </metadataConfiguration>
  <appSettings>
    <add key="vs:EnableBrowserLink" value="false" />
    <add key="vs:EnableBrowserLink" value="false" />
  </appSettings>
  <system.web>
    <authentication mode="None" />
    <compilation debug="true" targetFramework="4.5" />
    <httpRuntime targetFramework="4.5" maxRequestLength="404857" executionTimeout="3600" />   
  </system.web>
  <system.webServer>
    <validation validateIntegratedModeConfiguration="false" />
    <modules runAllManagedModulesForAllRequests="true"></modules>
    <security>
      <requestFiltering>
        <requestLimits maxAllowedContentLength="414573568" />
      </requestFiltering>
    </security>
    <handlers>
      <remove name="ExtensionlessUrlHandler-Integrated-4.0" />
      <remove name="OPTIONSVerbHandler" />
      <remove name="TRACEVerbHandler" />
      <add name="ExtensionlessUrlHandler-Integrated-4.0" path="*." verb="*" type="System.Web.Handlers.TransferRequestHandler" preCondition="integratedMode,runtimeVersionv4.0" />
    </handlers>
  </system.webServer>
  <runtime>
    <gcAllowVeryLargeObjects enabled="true" />    
